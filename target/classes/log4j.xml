<?xml version ="1.0" encoding ="UTF-8"?>
 <Configuration status ="INFO">
 <Appenders>
    <Console name="console" target ="SYSTEM_OUT">
        <JsonLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5 level logger {36} [%M] - %msg%n" />
    </Console>
 </Appenders>

 <Loggers>
    <Root level="trace">
        <AppenderRef ref="console" />
    </Root>
 </Loggers>
 </Configuration>